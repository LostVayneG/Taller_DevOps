pipeline {
    agent any
    stages{
        stage('build'){
            steps{
                sh 'cp -r rp-portfolio /deploy'
                sh 'apt-get install software-properties-common'
                sh 'add-apt-repository ppa:deadsnakes/ppa'
                sh 'apt-get update'
                sh 'apt-get install python3.8'
                sh 'python --version'
            }
        }
    }
}
