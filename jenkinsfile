pipeline {
    agent any
    stages{
        stage('config'){
            steps{
                sh 'cp -r ./rp-portfolio ./deploy/'
            }
        }
        stage('build'){
            steps{
                dir('./deploy/'){
                    sh 'python3 manage.py makemigrations projects'
                    sh 'python3 manage.py migrate projects'
                }
            }
        }
    }
}

