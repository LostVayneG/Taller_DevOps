pipeline {
    agent any
    stages{
        stage('config'){
            steps{
                sh 'cp -rf ./rp-portfolio ./deploy/'
            }
        }
        stage('build'){
            steps{
                sh 'python3 ./rp-portfolio/manage.py makemigrations projects'
                sh 'python3 ./rp-portfolio/manage.py migrate projects'
            }
        }
    }
}
